#Next {
    width: 100%;
    height: 100%;
}

#Next:hover {
    width: 120%;
    height: 120%;
    transition-duration: 500ms;
}

.contour {
    background-color: rgb(119, 245, 245);
}

.filling {
    position: absolute;
    left: 5px;
    top: 5px;
    right: 5px;
    bottom: 5px;
    background-color: rgb(9, 19, 16);
}

.pointStyle1000 {
    font-size: 60px;
}

.pointStyle {
    font-size: 75px;
}

.pointStyle10000 {
    font-size: 50px;
}

.MultiplicationIcon {
