.BackButton:focus {
    width: auto;
    height: auto;
    position: absolute;
    left: 20px;
    top: 20px;
    right: 20px;
    bottom: 20px;
    transition-duration: 0.25s;
}

.BackButton {
    position: absolute;
    left: 10px;
    top: 10px;
    right: 10px;
    bottom: 10px;
}
