#unity-tracker {
    color: rgb(17, 255, 0);
    background-color: rgba(68, 251, 185, 0);
    width: auto;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
}

#unity-dragger-border {
}

#unity-dragger {
    opacity: 1;
    background-image: none;
    -unity-background-image-tint-color: rgb(68, 251, 185);
    background-color: rgb(68, 251, 185);
    height: 76px;
    -unity-background-scale-mode: stretch-to-fill;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    top: auto;
    bottom: 2px;
}

#unity-drag-container {
    flex-direction: row;
    align-items: auto;
    justify-content: flex-start;
    overflow: hidden;
}

#unity-progress-bar {
    height: auto;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    position: absolute;
}

.bar {
    width: 2000px;
    height: 100%;
    background-color: rgba(68, 251, 185, 255);
    justify-content: flex-start;
    align-items: stretch;
    flex-basis: auto;
}
