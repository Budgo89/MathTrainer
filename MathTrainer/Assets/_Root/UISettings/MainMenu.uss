    right: 15px;
}

.RecordText {
    color: rgb(119, 245, 245);
    font-size: 50px;
    -unity-text-align: middle-left;
    -unity-font-style: bold;
}

.full {
    width: 320px;
    height: 80px;
    background-color: rgb(68, 251, 185);
    right: auto;
    left: 15px;
    -unity-text-align: middle-left;
    justify-content: center;
    align-items: center;
    flex-direction: row-reverse;
}

.text {
    color: rgb(2, 2, 2);
    font-size: 50px;
    -unity-text-align: middle-left;
    -unity-font-style: bold;
}

